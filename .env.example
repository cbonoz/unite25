ONE_INCH_API_KEY=

# Storacha configuration - Get these values by:
# 1. npm install -g @storacha/cli
# 2. storacha login your@email.com
# 3. storacha space create "your-space-name"
# 4. storacha delegation create <space-did> --can 'space/*' --can 'upload/*' --can 'store/*' --output proof.ucan
# 5. base64 encode the proof: base64 proof.ucan
NEXT_PUBLIC_STORACHA_PROOF=your_base64_encoded_proof_here
NEXT_PUBLIC_STORACHA_SPACE=your-space-name

NEXT_PUBLIC_APP_URL=

# Stellar Bridge Configuration
# Leave STELLAR_BRIDGE_SECRET_KEY empty for simulation mode in development
# For production, generate a Stellar keypair and fund it with assets
STELLAR_BRIDGE_SECRET_KEY=

# Stellar Network Configuration
# Will use testnet in development, mainnet in production based on NODE_ENV
